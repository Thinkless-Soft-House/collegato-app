{
  "expo": {
    "name": "com.thinkless.Dynamo.Collegatov2",
    "slug": "Dynamo-Collegato",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "jsEngine": "jsc",
    "plugins": [
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./assets/TopLogo.jpeg",
          "resizeMode": "cover"
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png"
        }
      ],
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "This app allows user to use Face ID to authenticate."
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "buildToolsVersion": "34.0.0"
          },
          "ios": {
            "deploymentTarget": "17.0"
          }
        }
      ],
      "expo-localization"
    ],
    "notification": {
      "color": "#FFF",
      "iosDisplayInForeground": true,
      "androidMode": "collapse",
      "androidCollapsedTitle": "Novas interações"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/4329dc4b-1c5c-4ca1-ba9b-496b82066331"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.thinkless.Dynamo.Collegatov2",
      "icon": "./assets/icon.png",
      "buildNumber": "3",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "This application allows the user to choose a photo to represent himself or his companies that are registered in the application.",
        "NSFaceIDUsageDescription": "This app allows user to use Face ID to authenticate."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "blockedPermissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION"
      ],
      "package": "com.thinkless.Dynamo.Collegatov2",
      "versionCode": 4,
      "permissions": [
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "extra": {
      "eas": {
        "projectId": "299ed39f-1e9e-4877-b0af-8b7ce667e6e3"
      }
    }
  }
}
